{
    "configurations": [
        {
            "name": "stm32f4_boot",
            "includePath": [
                "${workspaceFolder}/boot",
                "${workspaceFolder}/component/crc",
                "${workspaceFolder}/component/easylogger/inc",
                "${workspaceFolder}/component/ringbuffer",
                "${workspaceFolder}/platform/cmsis/core",
                "${workspaceFolder}/platform/cmsis/device",
                "${workspaceFolder}/platform/driver/inc"
            ],
            "defines": [
                "STM32F40_41xxx",
                "USE_STDPERIPH_DRIVER",
                "DEBUG"
            ],
            "compilerPath": "${workspaceFolder}/tools/toolchain/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gcc",
            "intelliSenseMode": "linux-gcc-arm",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
